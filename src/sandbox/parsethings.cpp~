
#include <iostream>
using namespace std;

//----------------------------------------------------------
// Procedure: Parse a string from a slave

bool parseIncomingReport(string report)
{
  vector<string> svec;
  vector<string> repvec = parseString(report, '#');
  for (i=2;i<repvec.size();i++){
    svec = parseString(repvec[i], ',');
    cout << repvec[i] << endl
    // for (j=0;j<svec.size();j++){
    //   cout << 
    // 	}
  }
}


int main ()
{
  string report;
  cout << "Input String: ";
  cin >> report;
  parseIncomingReport(report);
  cout << "The value you entered is " << i;
  cout << " and its double is " << i*2 << ".\n";
  return 0;
}
